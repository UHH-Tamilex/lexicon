        <cit type="nikantu" xml:id="Tiv8.89">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/67/mode/1up"><title xml:lang="ta">Tivākaram</title> 8.89 <term xml:lang="ta">viruppu</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>mātar, kātal, ātaravu, ārvam,</l>
                    <l>kāmar, veppam, paṟṟē, kāmam,</l>
                    <l>vīḻtal, vēṭkai, tāḻtal, veḵkal,</l>
                    <l>nacaiyē, nampu, mēvu, nayappu,</l>
                    <l>piṇaiyē, pēṇal, peṭpē, peṭṭal,</l>
                    <l>ivaṟal, avāvē, ivartal, kavartal,</l>
                    <l>viḻaivē, viruppiṉ (poruḷ kaṭṭuraiyē).</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu" xml:id="Tiv9.29">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/61/mode/1up"><title xml:lang="ta">Tivākaram</title> 9.29 <term xml:lang="ta">tāḻtal</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>iṟaiñcal, kuraṅkal, tāḻtal (ākum).</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu" xml:id="Tiv11.132">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/98/mode/1up"><title xml:lang="ta">Tivākaram</title> 11.132 <term xml:lang="ta">tūṅkal</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>tūṅkal, āṭalum, tāḻvum (collum).</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu" xml:id="Pin4.236">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/n194/mode/1up"><title xml:lang="ta">Piṅkalam</title> 4.236 <term xml:lang="ta">tāḻ</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>kāḻum, cīppum, kaṭikaiyum, tāḻ (ām).</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu" xml:id="Pin7.92">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/237/mode/1up"><title xml:lang="ta">Piṅkalam</title> 7.92 <term xml:lang="ta">tāḻvu</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>cāyppum, iḻivum, taṇmaiyum, tañcamum,</l>
                    <l>tāḻvu (eṉ kiḷavit taṉmaiyatu ākum).</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu" xml:id="Pin7.216">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/243/mode/1up"><title xml:lang="ta">Piṅkalam</title> 7.216 <term xml:lang="ta">vaṇaṅkal</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>vantaṉai, paṇital, taṇṭaṉ, vaṇakkam,</l>
                    <l>paraval, iṟaiñcal, tāḻtal, kāṇṭal,</l>
                    <l>vantittal, pōṟṟal, vaṇaṅkal (ākum).</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu" xml:id="Pin7.218">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/243/mode/1up"><title xml:lang="ta">Piṅkalam</title> 7.218 <term xml:lang="ta">tāḻtal</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>iṟaiñcalum, kuraṅkalum, tāḻtalai (icaikkum).</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu" xml:id="Pin10.508">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/304/mode/1up"><title xml:lang="ta">Piṅkalam</title> 10.508 <term xml:lang="ta">cīppu</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>kataviṉ tāḻum, kaṅkamum, cīppē.</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu" xml:id="Pin10.577">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/304/mode/1up"><title xml:lang="ta">Piṅkalam</title> 10.577 <term xml:lang="ta">taṇmai</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>tāḻvum, eḷimaiyum, taṇmai (eṉpa).</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu" xml:id="Pin10.602">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/308/mode/1up"><title xml:lang="ta">Piṅkalam</title> 10.602 <term xml:lang="ta">taḻmai</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>iḷivum, kīḻmaiyum, eṇmaiyum, tāḻmai.</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu" xml:id="Cut5.61">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/411/mode/1up"><title xml:lang="ta">Cūṭāmaṇi</title> 5.61 <term xml:lang="ta">paḷḷam, paḻaṅkollai, putuk kollai, uḻavu</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>iḻivu, kīḻ, payampu, tāḻvē,</l>
                    <l rend="indent">(ēṟṟiṭum) paṭukar, ñeḷḷal,</l>
                    <l>kuḻi, uvaḷakam, kiḻakku,</l>
                    <l rend="indent">(kūṟu) aval, paḷḷam (pattām);</l>
                    <l>aḻaiyatām kollaip (pērē),</l>
                    <l rend="indent">(pakar) mutaip puṉam (ām eṉpa);</l>
                    <l>(viḻaivuṟu) putiya kollai,</l>
                    <l rend="indent">itaip puṉam; uḻavu, toyyal.</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu" xml:id="Cut7.49">
            <ref target="https://archive.org/details/gc-sh10-0106/page/n107/mode/1up"><title xml:lang="ta">Cūṭāmaṇi</title> 7.49 <term xml:lang="ta">katavu, tāḻ, yāṉait tōṭṭi</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>kataviṉ (pēr), arari, vāri,</l>
                    <l rend="indent">kāppoṭu, kavāṭam, tōṭṭi,</l>
                    <l>patavum (ām); kāḻum, cīppum,</l>
                    <l rend="indent">(pōṟṟiya) kaṭikaiyum, tāḻ;</l>
                    <l>(matam uṟum) yāṉai (taṉṉai</l>
                    <l rend="indent">vaṇakkum) aṅkucamē, tōṭṭi,</l>
                    <l>(vitam uṟu) kōṇam (eṉṟum</l>
                    <l rend="indent">viḷampuvar aḷanta nūlōr).</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu" xml:id="Cut8.56">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/430/mode/1up"><title xml:lang="ta">Cūṭāmaṇi</title> 8.56 <term xml:lang="ta">accak kuṟippu, tāḻvu</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>(nilai peṟātu) ōṭal, tēṅkal,</l>
                    <l rend="indent">ñerēleṉal, kūcal, tiṭkal,</l>
                    <l>ulamaral, aḷukkal, ñoḷkal,</l>
                    <l rend="indent">(oṭu) tuṭukkeṉalē, tuṭkal,</l>
                    <l>kulaital, veytuṟalē, accak</l>
                    <l rend="indent">uṟippu, tuṇṇeṉalum (kūṟum);</l>
                    <l>talaiyiḻipuṭaṉē, cāyppu,</l>
                    <l rend="indent">tañcamē, taṇmai, tāḻvu.</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu" xml:id="Cut8.76">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/432/mode/1up"><title xml:lang="ta">Cūṭāmaṇi</title> 8.76 <term xml:lang="ta">vēṭkaip perukkam, kāmanōy</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>(cūḻtarum) mōkam, nampal,</l>
                    <l rend="indent">(coṟṟiṭum) irati, veḵkal,</l>
                    <l>tāḻtalē, kavartal, peṭṭal,</l>
                    <l rend="indent">kārpuṭaṉ, ivaṟal, vēṇṭal,</l>
                    <l>vīḻtal, vēṇavā, (īrāṟum)</l>
                    <l rend="indent">vēṭkaiyiṉ perukkam (eṉpa);</l>
                    <l>(māḻkiya) virakam, kāmam,</l>
                    <l rend="indent">mataṉamē, kāmanōy (ām).</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu" xml:id="Cut9.16">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/434/mode/1up"><title xml:lang="ta">Cūṭāmaṇi</title> 9.16 <term xml:lang="ta">aṭakkam, taṅkal, pāntal, tēyttal</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>muṭakkamē, maṭaṅkal, kutti,</l>
                    <l rend="indent">muṇaṅkoṭu, noṟil, oṭukkam,</l>
                    <l>aṭakkam (ām); vatital, vaikal,</l>
                    <l rend="indent">alkalē, iṟuttal, tāḻtal,</l>
                    <l>viṭuttal, (āṟum) taṅkal (pēr);</l>
                    <l rend="indent">vacittalōṭu, iṟaiyum (ap pēr);</l>
                    <l>oṭukkamē, patuṅkal, pāntal;</l>
                    <l rend="indent">uraittalē, uriñcal, tēyttal.</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu" xml:id="Cut9.19">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/435/mode/1up"><title xml:lang="ta">Cūṭāmaṇi</title> 9.19 <term xml:lang="ta">taṇṭaṉiṭal, āliṅkaṉam, kumpiṭal</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>vantaṉam, paṇital, kāṇal,</l>
                    <l rend="indent">vaḻipaṭal, paṇivu, iṟaiñcal,</l>
                    <l>(tam talai) vaṇaṅkal, pārttal,</l>
                    <l rend="indent">tāḻtal, (oṉpāṉum) taṇṭaṉ;</l>
                    <l>(muntiya) taḻuval, viḷḷal,</l>
                    <l rend="indent">muyaṅkal, āliṅkaṉap (pēr);</l>
                    <l>(antamāt) toḻal, kai kūppal,</l>
                    <l rend="indent">añcali, kumpiṭal (pēr).</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu" xml:id="Cut9.27">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/435/mode/1up"><title xml:lang="ta">Cūṭāmaṇi</title> 9.27 <term xml:lang="ta">aimpoṟi nutarvu, viṭuttal, aṭukkutal, oḻital, tāḻtal</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>(aṭutta) tuppuravum, tuvvum,</l>
                    <l rend="indent">aimpoṟi nukarvu (ākum);</l>
                    <l>viṭuttal, mōcaṉamē, mōkkam,</l>
                    <l rend="indent">vītamām, tīrtalum (pēr);</l>
                    <l>aṭukkutal (pērē), ēṟṟal;</l>
                    <l rend="indent">(ām) pval, oḻital (eṉpa);</l>
                    <l>(naṭukkuṟat) tāḻtaliṉ (pēr),</l>
                    <l rend="indent">kuraṅkutal, kuṉivum (nāṭṭum).</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu" xml:id="Cut9.40">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/437/mode/1up"><title xml:lang="ta">Cūṭāmaṇi</title> 9.40 <term xml:lang="ta">viraivuk kuṟippu, mantam</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>aiyeṉaloṭu, poḷḷeṉṟal,</l>
                    <l rend="indent">(āṉa) kuttiramē, caṇṭam,</l>
                    <l>noyteṉṟal, katumeṉṟiṭṭal,</l>
                    <l rend="indent">(noṭiyiṉil) porukkeṉṟiṭṭal,</l>
                    <l>veyteṉṟal, ñeḷḷal, kōram,</l>
                    <l rend="indent">viraiviṉil kuṟippu (pattām);</l>
                    <l>meyyeṉakoṭu, melleṉṟal,</l>
                    <l rend="indent">paiyeṉal, tāḻttal, mantam.</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu" xml:id="Cut11.23">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/448/mode/1up"><title xml:lang="ta">Cūṭāmaṇi</title> 11.23 <term xml:lang="ta">ñāṅkar, tūṅkal, vāṅkal, vai, vēṅkai, vī</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>ñāṅkar, vēl, pakkam, muṉpu,</l>
                    <l rend="indent">mēl (eṉa nāṉkum ākum);</l>
                    <l>tūṅkalē, niruttam, yāṉai,</l>
                    <l rend="indent">tuyil, cōmpu, tarācu, tāḻtal;</l>
                    <l>vāṅkalē, vaḷattal, koḷḷal;</l>
                    <l rend="indent">vai, (eṉpa) kūrmai, vaikkōl;</l>
                    <l>vēṅkai, poṉ, puli, marap(pēr);</l>
                    <l rend="indent">vī, nīkkam, pū, puḷ, cā (ām).</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu" xml:id="Cut11.49">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/451/mode/1up"><title xml:lang="ta">Cūṭāmaṇi</title> 11.49 <term xml:lang="ta">kaṭikai, paṭiṟu, pakartal, cuṭikai, iṭi</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>kaṭikai, nāḻikaiyē, tuṇṭam,</l>
                    <l rend="indent">katavu iṭu tāḻē (mup pēr);</l>
                    <l>paṭiṟu, poy, kaḷavu (iraṇṭu ām);</l>
                    <l rend="indent">pakartalē, viṟṟal, pēcal;</l>
                    <l>cuṭikaiyē, makuṭam, ucci,</l>
                    <l rend="indent">cuṭṭiyum (iṭṭa pērē);</l>
                    <l>iṭi, iṭip pērē, nel mā,</l>
                    <l rend="indent">(ēṟṟiṭum) uṟutic collē.</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu" xml:id="Cut11.71">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/453/mode/1up"><title xml:lang="ta">Cūṭāmaṇi</title> 11.71 <term xml:lang="ta">paṭalikai, koṭiyaṉ, kuṇṭu, toṭu, muṭi</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>paṭalikai, pīrkku, vaṭṭam,</l>
                    <l rend="indent">(paranta) kaim maṇi, pūn taṭṭu (ām);</l>
                    <l>koṭiyaṉē, koṭiyaṉ, kētu (ām);</l>
                    <l rend="indent">kuṇṭu, āḻam, tāḻvu, āṇ māvē;</l>
                    <l>toṭu (eṉpa), marutap pūmi,</l>
                    <l rend="indent">tōṭṭam, kaitavam (mup pērē);</l>
                    <l>muṭi, mayir muṭiyē, ceṉṉi,</l>
                    <l rend="indent">makuṭamum (moḻiyalāmē).</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu" xml:id="Cut11.135">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/460/mode/1up"><title xml:lang="ta">Cūṭāmaṇi</title> 11.135 <term xml:lang="ta">cīppu, nāppaṇ, kāppu, yāppu, iṟāl</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>cīppu (eṉpa), kataviṉ tāḻum,</l>
                    <l rend="indent">cīvu kaṅkamum (ām eṉpa);</l>
                    <l>nāppaṇ (eṉpatu), tērt taṭṭum,</l>
                    <l rend="indent">naṭuvum, yāḻuṟuppum (muppēr);</l>
                    <l>kāppu (eṉpa), kāvalōṭu,</l>
                    <l rend="indent">katavum, veṇṇīṟum (ākum);</l>
                    <l>yāppu (eṉpa), kavitai, kaṭṭu (ām);</l>
                    <l rend="indent">iṟāl, tēṉ kūṭu, erutu, mīṉē.</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu" xml:id="Cut11.245">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/n470/mode/1up"><title xml:lang="ta">Cūṭāmaṇi</title> 11.245 <term xml:lang="ta">pavam, pāvam, cavuriyam, kaṇmai, yavam, yavaṉar</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>pavam (eṉpa), caṉaṉam, pāvam;</l>
                    <l rend="indent">pāvam (tāṉ), viṉai, tiyāṉam;</l>
                    <l>cavuriyam, kaḷavu, vīram;</l>
                    <l rend="indent">taṇmai (tāṉ), kuḷircci, tāḻvu;</l>
                    <l>yavam, oru tāṉiyap (pēr),</l>
                    <l rend="indent">(eṉpa), nel(liṟkum ap pēr);</l>
                    <l>yavaṉar, cōṉakar, kaṇ-</l>
                    <l rend="indent">ṇāḷar, cittirakārar (eṉpa).</l>
                </lg>
            </q>
        </cit>
        <cit type="external">
            <ref target="https://www.ifpindia.org/digitaldb/site/digital_tevaram/U_TEV/CT-TAA.HTM#:~:text=y%C4%93y%C4%81m%C4%81%20t%C4%81v%C4%93%E1%B8%BB%C4%ABk%C4%81%20n%C4%ABt%C4%81%E1%B9%89%C4%93-,t%C4%81%E1%B8%BB,-%3A%2022">Digital <term xml:lang="ta">Tēvāram</term> 98.182</ref>
