        </cit>
        <cit type="nikantu">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/434/mode/1up"><title xml:lang="ta">Cūṭāmaṇi</title> 9.16 <term xml:lang="ta">aṭakkam, taṅkal, pāntal, tēyttal</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>muṭakkamē, maṭaṅkal, kutti,</l>
                    <l rend="indent">muṇaṅkoṭu, noṟil, oṭukkam,</l>
                    <l>aṭakkam (ām); vatital, vaikal,</l>
                    <l rend="indent">alkalē, iṟuttal, tāḻtal,</l>
                    <l>viṭuttal, (āṟum) taṅkal (pēr);</l>
                    <l rend="indent">vacittalōṭu, iṟaiyum (ap pēr);</l>
                    <l>oṭukkamē, patuṅkal, pāntal;</l>
                    <l rend="indent">uraittalē, uriñcal, tēyttal.</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/437/mode/1up"><title xml:lang="ta">Cūṭāmaṇi</title> 9.40 <term xml:lang="ta">viraivuk kuṟippu, mantam</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>aiyeṉaloṭu, poḷḷeṉṟal,</l>
                    <l rend="indent">(āṉa) kuttiramē, caṇṭam,</l>
                    <l>noyteṉṟal, katumeṉṟiṭṭal,</l>
                    <l rend="indent">(noṭiyiṉil) porukkeṉṟiṭṭal,</l>
                    <l>veyteṉṟal, ñeḷḷal, kōram,</l>
                    <l rend="indent">viraiviṉil kuṟippu (pattām);</l>
                    <l>meyyeṉakoṭu, melleṉṟal,</l>
                    <l rend="indent">paiyeṉal, tāḻttal, mantam.</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/435/mode/1up"><title xml:lang="ta">Cūṭāmaṇi</title> 9.19 <term xml:lang="ta">taṇṭaṉiṭal, āliṅkaṉam, kumpiṭal</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>vantaṉam, paṇital, kāṇal,</l>
                    <l rend="indent">vaḻipaṭal, paṇivu, iṟaiñcal,</l>
                    <l>(tam talai) vaṇaṅkal, pārttal,</l>
                    <l rend="indent">tāḻtal, (oṉpāṉum) taṇṭaṉ;</l>
                    <l>(muntiya) taḻuval, viḷḷal,</l>
                    <l rend="indent">muyaṅkal, āliṅkaṉap (pēr);</l>
                    <l>(antamāt) toḻal, kai kūppal,</l>
                    <l rend="indent">añcali, kumpiṭal (pēr).</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/435/mode/1up"><title xml:lang="ta">Cūṭāmaṇi</title> 9.27 <term xml:lang="ta">aimpoṟi nutarvu, viṭuttal, aṭukkutal, oḻital, tāḻtal</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>(aṭutta) tuppuravum, tuvvum,</l>
                    <l rend="indent">aimpoṟi nukarvu (ākum);</l>
                    <l>viṭuttal, mōcaṉamē, mōkkam,</l>
                    <l rend="indent">vītamām, tīrtalum (pēr);</l>
                    <l>aṭukkutal (pērē), ēṟṟal;</l>
                    <l rend="indent">(ām) pval, oḻital (eṉpa);</l>
                    <l>(naṭukkuṟat) tāḻtaliṉ (pēr),</l>
                    <l rend="indent">kuraṅkutal, kuṉivum (nāṭṭum).</l>
                </lg>
            </q>
        </cit>
        <cit type="nikantu">
            <ref target="https://archive.org/details/dli.jZY9lup2kZl6TuXGlZQdjZp7kZUy.TVA_BOK_0002448/page/448/mode/1up"><title xml:lang="ta">Cūṭāmaṇi</title> 11.23 <term xml:lang="ta">ñāṅkar, tūṅkal, vāṅkal, vai, vēṅkai, vī</term></ref>
            <q rend="block" xml:lang="ta">
                <lg>
                    <l>ñāṅkar, vēl, pakkam, muṉpu,</l>
                    <l rend="indent">mēl (eṉa nāṉkum ākum);</l>
                    <l>tūṅkalē, niruttam, yāṉai,</l>
                    <l rend="indent">tuyil, cōmpu, tarācu, tāḻtal;</l>
                    <l>vāṅkalē, vaḷattal, koḷḷal;</l>
                    <l rend="indent">vai, (eṉpa) kūrmai, vaikkōl;</l>
                    <l>vēṅkai, poṉ, puli, marap(pēr);</l>
                    <l rend="indent">vī, nīkkam, pū, puḷ, cā (ām).</l>
                </lg>
            </q>
        </cit>
        <cit type="external">
            <ref target="https://www.ifpindia.org/digitaldb/site/digital_tevaram/U_TEV/CT-TAA.HTM#:~:text=y%C4%93y%C4%81m%C4%81%20t%C4%81v%C4%93%E1%B8%BB%C4%ABk%C4%81%20n%C4%ABt%C4%81%E1%B9%89%C4%93-,t%C4%81%E1%B8%BB,-%3A%2022">Digital <term xml:lang="ta">Tēvāram</term> 98.182</ref>
        </cit>
